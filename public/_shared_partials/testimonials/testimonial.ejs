<div class="col s12 testimonial">
  <blockquote>
    <div class="heading">&quot;<%- heading %>&quot;</div>
    <div class="quote"><%- text %></div>
    <div class="source">
      <div class="source-image">
        <img class="circle" src="<%= image %>">
      </div>
      <div class="source-person">
        <b><%= name %><%= company %></b><br><%= location %>
      </div>
    </div>
  </blockquote>
</div>
